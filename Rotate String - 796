/**
 * @param {string} s
 * @param {string} goal
 * @return {boolean}
 */
var rotateString = function(s, goal) {
    // console.log(s.substring(0,s.length) + s.substring(0,1) );

    for(let i = 1 ; i < s.length ; i++){
        if((s.substring(i,s.length) + s.substring(0,i)) == goal){
            return true;
        }

        
    }
    return false;
};
