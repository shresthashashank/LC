/**
 * @param {number[]} nums
 * @return {number}
 */
var maxSubArray = function(nums) {
    let maxSum = nums[0];
    let current_sum = maxSum;
    
    for(let i = 1; i < nums.length; i++){
        current_sum = Math.max(nums[i] + current_sum , nums[i]);
        maxSum = Math.max(current_sum , maxSum);
    }
    return maxSum;
};
